{
  "name": "acwing",
  "displayName": "acwing",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:acwing.gotoPage",
    "onCommand:acwing.previewProblem",
    "onCommand:acwing.showProblem",
    "onView:acWing"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ac-problem-explorer",
          "title": "AcWing",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "ac-problem-explorer": [
        {
          "id": "acWing",
          "name": "AcWing",
          "icon": "media/dep.svg",
          "contextualTitle": "AcWing"
        }
      ]
    },
    "commands": [
      {
        "command": "acWing.refreshEntry",
        "title": "刷新",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "acWing.prevPage",
        "title": "上一页",
        "icon": "$(triangle-right)"
      },
      {
        "command": "acWing.nextPage",
        "title": "下一页",
        "icon": "$(triangle-left)"
      },
      {
        "command": "acWing.configure",
        "title": "配置",
        "icon": "$(settings-gear)"
      },
      {
        "command": "acWing.gotoPage",
        "title": "跳转页面",
        "icon": "$(reply)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "acWing.refreshEntry",
          "when": "view == acWing",
          "group": "navigation@4"
        },
        {
          "command": "acWing.gotoPage",
          "when": "view == acWing",
          "group": "navigation@3"
        },
        {
          "command": "acWing.prevPage",
          "when": "view == acWing",
          "group": "navigation@2"
        },
        {
          "command": "acWing.nextPage",
          "when": "view == acWing",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.189",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.73.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/request-promise": "^4.1.48",
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ws": "^8.11.0"
  }
}
